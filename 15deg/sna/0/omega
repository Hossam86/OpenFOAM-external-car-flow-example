FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 0.057;

boundaryField
{
    outlet
    {
        type            zeroGradient;
    }
    outdoors
    {
        type            symmetryPlane;
    }
    floor
    {
	type		slip;
    }
    inlet
    {
        type            fixedValue;
        value           uniform 13.333;
    }
    "fixedWalls.*"
    {
        type            omegaWallFunction;
        Cmu             0.09;
        kappa           0.41;
        E               9.8;
        beta1           0.075;
        value           uniform 0.057;
    }
}


